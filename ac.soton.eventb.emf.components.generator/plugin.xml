         </rule> 
         <rule
               name="ChildComponentRule"
               rule_class="ac.soton.eventb.emf.components.generator.rules.ChildComponentRule"
               source_class="Component">
         </rule>
         <rule
               name="WakeQueueRule"
               rule_class="ac.soton.eventb.emf.components.generator.rules.WakeQueueRule"
